package reversenumber;

import java.util.Scanner;

public class SeriesPalindrome {
public class SeriesofPalindrome extends ReverseNumbers{
	private int lw,up;
	private Object scan;
	private void ScanRange()
	{
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter lw,up values");
		lw=scan.nextInt();
		up=scan.nextInt();
	}

public void displaySeriesofPalindrome() {
	this.ScanRange();
	for(int i=lw;i<=up;i++)
	{
		int rev1=this.getReverseofNumbers(i);
		boolean status=this.ispalindrome(i, rev1);
		this.displaySeriesofPalindrome();
	}
}
}

	
}
}


	


